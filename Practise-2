End If

End Sub

Private Sub CommandButton15_Click()

Dim a, b As Integer

a = TextBox1.Value
b = TextBox2.Value
c = TextBox5.Value

If TextBox3.Value = "+" And TextBox5.Text = "" Then
    TextBox4.Text = a + b
ElseIf TextBox3.Value = "+" And TextBox5.Text <> "" Then
    TextBox4.Text = c + b
ElseIf TextBox3.Value = "-" And TextBox5.Text = "" Then
    TextBox4.Text = a - b
ElseIf TextBox3.Value = "-" And TextBox5.Text <> "" Then
    TextBox4.Text = c - b
ElseIf TextBox3.Value = "*" And TextBox5.Text = "" Then
    TextBox4.Text = a * b
ElseIf TextBox3.Value = "*" And TextBox5.Text <> "" Then
    TextBox4.Text = c * b
ElseIf TextBox3.Value = "/" And TextBox5.Text = "" Then
    TextBox4.Text = a / b
ElseIf TextBox3.Value = "/" And TextBox5.Text <> "" Then
    TextBox4.Text = c / b
End If

TextBox5.Text = TextBox4.Value

End Sub

Private Sub CommandButton11_Click()

If TextBox5.Value = "" Then
    TextBox3.Value = "+"
Else: TextBox2.Value = ""
    TextBox3.Value = "+"
End If

End Sub

Private Sub CommandButton12_Click()

If TextBox5.Value = "" Then
    TextBox3.Value = "-"
Else: TextBox2.Value = ""
    TextBox3.Value = "-"
End If

End Sub

Private Sub CommandButton13_Click()

If TextBox5.Value = "" Then
    TextBox3.Value = "*"
Else: TextBox2.Value = ""
    TextBox3.Value = "*"
End If

End Sub

Private Sub CommandButton14_Click()

If TextBox5.Value = "" Then
    TextBox3.Value = "/"
Else: TextBox2.Value = ""
    TextBox3.Value = "/"
End If

End Sub
