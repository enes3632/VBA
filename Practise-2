    TextBox1.Value = TextBox1.Value & "0"
ElseIf TextBox1.Text <> "" And TextBox2.Text = "" And TextBox3.Text <> "" Then
    TextBox2.Value = 0
ElseIf TextBox1.Text <> "" And TextBox2.Text <> "" And TextBox3.Text <> "" Then
    TextBox2.Value = TextBox2.Value & "0"
End If

If TextBox3.Text = "" And TextBox5.Text = "" Then
    TextBox4.Value = TextBox4.Value & "0"
ElseIf TextBox3.Text <> "" And TextBox5.Text = "" Then
    TextBox4.Value = TextBox1.Value & TextBox3.Value & TextBox2.Value
ElseIf TextBox3.Text <> "" And TextBox5.Text <> "" Then
