
Private Sub CommandButton16_Click()

TextBox1.Value = ""
TextBox2.Value = ""
TextBox3.Value = ""
TextBox4.Value = ""
TextBox5.Value = ""

End Sub

Private Sub CommandButton1_Click()

If TextBox1.Text = "" And TextBox3.Text = "" Then
    TextBox1.Value = 1
ElseIf TextBox1.Text <> "" And TextBox3.Text = "" Then
    TextBox1.Value = TextBox1.Value & "1"
ElseIf TextBox1.Text <> "" And TextBox2.Text = "" And TextBox3.Text <> "" Then
    TextBox2.Value = 1
ElseIf TextBox1.Text <> "" And TextBox2.Text <> "" And TextBox3.Text <> "" Then
    TextBox2.Value = TextBox2.Value & "1"
End If

If TextBox3.Text = "" And TextBox5.Text = "" Then
    TextBox4.Value = TextBox4.Value & "1"
ElseIf TextBox3.Text <> "" And TextBox5.Text = "" Then
    TextBox4.Value = TextBox1.Value & TextBox3.Value & TextBox2.Value
ElseIf TextBox3.Text <> "" And TextBox5.Text <> "" Then
    TextBox4.Value = TextBox5.Value & TextBox3.Value & TextBox2.Value
End If

End Sub

Private Sub CommandButton2_Click()

If TextBox1.Text = "" And TextBox3.Text = "" Then
